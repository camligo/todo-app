{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer"
      },
      "name": {
        "type": "string"
      },
      "createdAt": {
        "type": "string"
      },
      "updatedAt": {
        "type": "string"
      },
      "dueDate": {
        "type": "string"
      },
      "priority": {
        "type": "boolean"
      },
      "archived": {
        "type": "boolean"
      }
    },
    "required": ["id", "name", "createdAt", "updatedAt"]
  }
}
